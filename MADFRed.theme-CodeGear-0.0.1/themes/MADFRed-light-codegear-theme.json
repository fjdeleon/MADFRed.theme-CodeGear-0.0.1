  "tokenColors": [
    {
      "name": "Text",
      "scope": ["comment.unused.elixir", "text.html.derivated"],
      "settings": {
        "foreground": "#1e4357"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment.block.documentation",
        "comment.block.documentation punctuation.definition"
      ],
      "settings": {
        "foreground": "#555500"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "comment.block"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6F8A0EC9" /*  comentarios */,
      }
    },
    {
      "name": "Comments",
      "scope": ["punctuation.definition.comment", "string.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#C4C400" /*  <!-- --> */
      }
    },
    {
      "name": "Comments",
      "scope": ["comment.block.documentation"],
      "settings": {
        "foreground": "#CE151E"
      }
    },
    {
      "name": "Doc Comment Keywords",
      "scope": [
        "storage.type.class.jsdoc",
        "keyword.other.documentation",
        "keyword.other.phpdoc",
        "meta.other.type.phpdoc support class",
        "comment.block.documentation variable",
        "comment.block variable.parameter",
        "comment.block.documentation entity.name.type"
      ],
      "settings": {
        "foreground": "#557947" /*   */
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.comma.graphql",
        "punctuation.definition.variable",
        "punctuation.definition.parameters",
        "punctuation.definition.array",
        "punctuation.definition.function",
        "punctuation.brace",
        "punctuation.terminator.statement",
        "punctuation.delimiter.object.comma",
        "punctuation.definition.entity",
        "punctuation.definition",
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown",
        "punctuation.separator.key-value",
        "punctuation.separator.dictionary",
        "punctuation.terminator",
        "punctuation.delimiter.comma",
        "punctuation.separator.comma",
        "punctuation.accessor",
        "punctuation.separator.array",
        "punctuation.section",
        "punctuation.section.property-list.begin.bracket.curly",
        "punctuation.section.property-list.end.bracket.curly",
        "punctuation.separator.statement",
        "punctuation.section.array.elixir",
        "punctuation.separator.object.elixir",
        "punctuation.section.embedded.elixir",
        "punctuation.section.function.elixir",
        "punctuation.section.scope.elixir",
        "punctuation.separator.parameter",
        "meta.brace.round",
        "meta.brace.square",
        "meta.brace.curly",
        "constant.name.attribute.tag.pug",
        "punctuation.section.embedded",
        "punctuation.separator.method",
        "punctuation.separator",
        "punctuation.other.comma",
        "punctuation.bracket",
        "keyword.control.ternary",
        "string.interpolated.pug",
        "support.function.interpolation.sass",
        "punctuation.parenthesis.begin",
        "punctuation.parenthesis.end",
        "punctuation.operation.graphql",
        "punctuation.colon.graphql"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffef64bd"
      }
    },
    {
      "name": "Delimiters",
      "scope": "none",
      "settings": {
        "foreground": "#ffe68a"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#fea876"
      }
    },

    {
      "name": "Primitive Type",
      "scope": "support.type.primitive",
      "settings": {
        "foreground": "#3286ad"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "source.elixir.embedded.source",
        "string source.groovy",
        "string meta.embedded.line.ruby"
      ],
      "settings": {
        "foreground": "#FF00EE" /* JS Variable local - variable estaba en este bloque */
      }
    },
    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": {
        "foreground": "#188181" /* JS Variable local ------- save.     */
      }
    },
    {
      "name": "Variables" /* meta.function.call solo toma el italic y no el foreground*/,
      "scope": ["meta.function-call"],
      "settings": {
        "fontStyle": "italic" /* JS Variable global document    */
      }
    },

    {
      "name": "Object Properties",
      "scope": [
        "meta.object-literal.key",
        "meta.object.member",
        "variable.other.property",
        "variable.other.object.property",
        "support.variable.property",
        "variable.object.property",
        "entity.name.tag.yaml",
        "constant.other.key",
        "constant.other.object.key.js",
        "string.unquoted.label.js",
        "support.type.map.key",
        "variable.graphql"
      ],
      "settings": {
        "foreground": "#6DADB1" /* JS propiedad  .href .target cf532d */
      }
    },
    {
      "name": "Variable Parameter",
      "scope": [
        "variable.parameter",
        "parameter.variable.function.elixir",
        "variable.other.block.ruby"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6974A4" /* parametro.propiedad.propiedad "event" .target.result 2191b3*/
      }
    },
    {
      "name": "Constants",
      "scope": [
        "punctuation.definition.constant",
        "constant.other.symbol",
        "constant.language.symbol",
        "support.constant",
        "support.variable.magic.python"
      ],
      "settings": {
        "foreground": "#7E5304" /* target.result; */
      }
    },
    {
      "name": "Constants",
      "scope": ["support.constant.color.w3c-extended-color-name"],
      "settings": {
        "foreground": "#584011" /* colores nombrados */
      }
    },
    {
      "name": "Constants",
      "scope": ["constant", "variable.other.constant"],
      "settings": {
        "foreground": "#8A4710" /* JS constantes */
      }
    },

    {
      "name": "Object Properties",
      "scope": ["meta.property-name", "meta.property-list"],
      "settings": {
        "foreground": "#73157C" /* no se */
      }
    },
    {
      "name": "Object Properties",
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#1C5C68" /* name scope settings */
      }
    },
    {
      "name": "Strings, Inherited Class",
      "scope": ["support.constant.property-value"],
      "settings": {
        "foreground": "#13B39DAB" /* display: --> flex <-- valor de atributo? */
      }
    },
    {
      "name": "Attribute IDs",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#049E9E" /*  HTML - charset lang name     */
      }
    },
    {
      "name": "Attribute IDs",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#2048B6" /*  CSS - def class en css .columna     */
      }
    },
    {
      "name": "Attribute IDs",
      "scope": ["string.unquoted.alias.graphql"],
      "settings": {
        "foreground": "#006060" /*  Atributo  */
      }
    },

    {
      "name": "Functions",
